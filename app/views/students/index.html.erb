<h1>
    Listing all Students
    <table>
        <thead>
            <tr>
                <th>First name </th>
                <th>Last name </th>
                <th>Email </th>

            </tr>
        </thead>
        <tbody>
            <% @students.each do |student| %>
            <tr>
                <td><%= student.first_name %></td>
                <td><%= student.last_name %></td>
                <td><%= student.email %></td>
            </tr>
            <%end%>
        </tbody>

    </table>
</h1>